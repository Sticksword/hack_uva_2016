{
	"manifest_version": 2,

	"name": "Event_scrapper",
	"description": "",
	"version": "0.2",
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"css": ["bootstrap.css", "bootstrap.min.css", 
			"bootstrap-theme.css", "bootstrap-theme.min.css"],
			"js": ["jquery.js"]
		}
	],
	"browser_action": {
		"default_icon": "icon.jpg",
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab"
	]
}